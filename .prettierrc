{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto",
  "parser": "typescript",
  "importOrder": [
    "^@nestjs/(.*)$",
    "^@decorators/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@common/(.*)$",
    "^@modules/(.*)$",
    "^@filters/(.*)$",
    "^@pipes/(.*)$",
    "^@swagger/(.*)$",
    "^@validators/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}
